<?xml version="1.0"?>

<object name="portal_skins">

  <object name="themecentre_images" meta_type="Filesystem Directory View"
          directory="eea.themecentre:skins/themecentre_images"/>
  <object name="themecentre_styles" meta_type="Filesystem Directory View"
          directory="eea.themecentre:skins/themecentre_styles"/>
  <object name="themecentre_templates" meta_type="Filesystem Directory View"
          directory="eea.themecentre:skins/themecentre_templates"/>

  <skin-path name="*">
   <layer name="themecentre_images" insert-after="custom" />
  </skin-path>
  <skin-path name="*">
   <layer name="themecentre_styles" insert-after="custom" />
  </skin-path>
  <skin-path name="*">
   <layer name="themecentre_templates" insert-after="custom" />
  </skin-path>

</object>
